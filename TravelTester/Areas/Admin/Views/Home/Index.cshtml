@model IEnumerable<TravelTester.Areas.Admin.Models.User>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">FirstName</th>
            <th scope="col">LastName</th>
            <th scope="col">Email</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <th scope="row"> @Html.DisplayFor(modelItem => item.Id)</th>
                <td>  @Html.DisplayFor(modelItem => item.FirstName)</td>
                <td>  @Html.DisplayFor(modelItem => item.LastName)</td>
                <td>  @Html.DisplayFor(modelItem => item.Email)</td>
                <td>
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Edit", "Edit", "Users", new { id = item.Id }, null) |
                    @Html.ActionLink("Delete", "Delete", "Users", new { id = item.Id }, null)

                </td>
            </tr>
        }
    </tbody>
</table>